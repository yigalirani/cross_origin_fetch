{
  "name": "cross origin fetch",
  "version": "1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "service-worker.js"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "host_permissions": ["*://*/*"],
  "permissions": [
    "sidePanel",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"    
  ]
}